;; shadow-cljs configuration
{:lein {:profile "+client"}
 :nrepl {:port 8000}
 :builds {:client {:target :browser
                   :output-dir "resources/public/assets/js"
                   :asset-path "/assets/js"
                   :modules {:title {:entries [lilactown.client.title.core]}}
                   :devtools {:before-load lilactown.client.title.core/stop
                              :after-load lilactown.client.title.core/start}}}}
